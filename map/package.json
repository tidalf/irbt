{
  "name": "robmap",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/d3": "^5.7.2",
    "d3": "^5.12.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.11.0",
    "yarn": ">=1.19.0"
  },
  "scripts": {
    "lint": "tslint -p .",
    "build": "yarn audit && yarn lint && tsc --pretty && yarn bundle",
    "bundle": "rollup -c",
    "watch:transpile": "tsc --pretty -w",
    "watch:bundle": "rollup -c --watch",
    "start": "yarn build; yarn open",
    "clean": "rm -rf build/ node_modules/ html/js/robmap.js",
    "open": "open ./html/index.htm",
    "srv:test": "cd html; python3 -m http.server --cgi 8000"
  },
  "devDependencies": {
    "rollup-plugin-terser": "^5.1.2",
    "node": "^12.11.1",
    "rollup": "^1.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}